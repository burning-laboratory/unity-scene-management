{
   "name": "com.burning-lab.scenemanagement",
   "version": "0.1.10",
   "displayName": "Scene Management",
   "description": "Async scene management system.",
   "unity": "2021.3",
   "documentationUrl": "https://burning-lab.youtrack.cloud/articles/SM-A-1/Scene-Management-Dokumentatsiya",
   "changelogUrl": "https://github.com/burning-laboratory/unity-scene-management/releases",
   "licensesUrl": "https://github.com/burning-laboratory/unity-scene-management/blob/main/LICENSE.txt",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/burning-laboratory/unity-scene-management"
   },
   "keywords": [
      "Unity",
      "Scene",
      "Management"
   ],
   "scripts": {
      "build-readme": "node ./node_modules/unity-build-package-utils/ReadmeControl/BuildReadme.js -config ./unity-package-config.json",
      "set-package-version-suffix": "node ./node_modules/unity-build-package-utils/VersionControl/SetVersionSuffix.js -config ./unity-package-config.json",
      "clear-package-version-suffix": "node ./node_modules/unity-build-package-utils/VersionControl/ClearVersionSuffix.js -config ./unity-package-config.json",
      "save-unity-dependencies": "node ./node_modules/unity-build-package-utils/DependenciesControl/SaveUnityDependencies.js -config ./unity-package-config.json",
      "load-unity-dependencies": "node ./node_modules/unity-build-package-utils/DependenciesControl/LoadUnityDependencies.js -config ./unity-package-config.json",
      "restore-unity-dependencies": "node ./node_modules/unity-build-package-utils/DependenciesControl/RestoreUnityDependencies.js -config ./unity-package-config.json",
      "process-unity-samples": "node ./node_modules/unity-build-package-utils/SamplesControl/ProcessSamples.js -config ./unity-package-config.json",
      "restore-unity-samples": "node ./node_modules/unity-build-package-utils/SamplesControl/RestoreSamples.js -config ./unity-package-config.json"
   },
   "author": {
      "name": "Burning-Lab",
      "email": "packages.support@burning-lab.com",
      "url": "https://burning-lab.com"
   },
   "devDependencies": {
      "unity-build-package-utility": "^0.0.1"
   },
   "samples": [
      {
         "displayName": "Tutorial app",
         "description": "Scene management system tutorial application.",
         "path": "Samples/SM Tutorial Application"
      }
   ],
   "unityDependencies": {
      "com.unity.addressables": "1.19.19"
   }
}